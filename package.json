{
  "name": "sci-parser",
  "private": false,
  "version": "1.0.0",
  "author": {
    "name": "Mart√≠n Schiaffino"
  },
  "license": "MIT",
  "description": "SCI parser based on Genex module (by Alix Axel)",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc",
    "postversion": "git push && git push --tags && npm publish",
    "test": "jest",
    "version": "git add -A ."
  },
  "dependencies": {
    "ret": "^0.3.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.3",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mschiaffino/sci-parser.git"
  },
  "keywords": [
    "sci"
  ],
  "jest": {
    "roots": [
      "tests/"
    ],
    "testRegex": "[.]test[.]tsx?$",
    "transform": {
      "[.]tsx?$": "ts-jest"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }
}
